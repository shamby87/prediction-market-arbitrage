# CLOB HTTP endpoint
CLOB_HOST=https://clob.polymarket.com

# Polygon chain id
CHAIN_ID=137

# Your Polygon private key that controls the funds (no quotes, no 0x prefix or with, just be consistent with ethers)
POLY_PRIV_KEY=your_priv_key   # same as PRIVATE_KEY in Polymarket docs

# Your Polymarket profile / funder address (proxy wallet where you deposit USDC)
PROFILE_ADDR=your_profile_address

KALSHI_API_KEY_ID=your_kalshi_api_key_id
KALSHI_PRIVATE_KEY_PATH=/path/to/kalshi/private/key

# Optional: override WebSocket URL if needed; default is fine for now
# POLY_WS_MARKET_URL=wss://ws-subscriptions-clob.polymarket.com/ws/market

# Optional: minimum edge for YES+NO arbitrage
MIN_EDGE=0.001

DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/your/webhook
DISCORD_NOTIFY_USER_ID=your_user_id